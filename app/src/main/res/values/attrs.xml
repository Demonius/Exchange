<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string|reference" />
    <attr name="flowSuffixText" format="string|reference" />
    <declare-styleable name="TextInputView">
        <attr name="title" format="string|reference" />
        <attr name="text" />

        <attr name="prefixText" format="string|reference" />
        <attr name="suffixText" format="string|reference" />
        <attr name="flowSuffixText" />

        <attr name="errorEnabled" format="boolean|reference" />
        <attr name="errorText" format="string|reference" />

        <attr name="android:inputType" />
        <attr name="android:hint" />
        <attr name="android:imeOptions" />
        <attr name="android:imeActionId" />
        <attr name="android:textAppearance" />
        <attr name="android:textStyle" />
        <attr name="android:gravity" />
        <attr name="android:textSize" />
    </declare-styleable>
